{
    "manifest_version": 3,
    "name": "Print Helper",
    "version": "1.0",
    "description": "A simple extension to print pages with unnecessary elements removed.",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon48.png"
    },
    "options_page": "settings.html",
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    }
  }